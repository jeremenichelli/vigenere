    "babelify": "^7.2.0",
    "del": "^2.2.0",
    "eslint": "^1.10.3",
    "jasmine-core": "^2.4.1",
    "karma": "^0.13.19",
    "karma-browserify": "^4.4.2",
    "karma-jasmine": "^0.3.6",
    "karma-phantomjs-launcher": "^0.2.3",
    "karma-spec-reporter": "0.0.23",
    "phantomjs": "^1.9.19"
